<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m"
>
    <Dialog
        id="idEditInssAddEquipmentDialog"
        resizable="true"
        draggable="true"
        contentWidth="40%"
        contentHeight="37%"
        title="Edit Vehicle"
    >
        <beginButton>
            <Button
                id="idCreatessvgbhdfgjkButton"
                text="Save"
                press="onSave"
                type="Success"
            />
        </beginButton>
        <endButton>
            <Button
                id="idCancelssbnmdfgButton"
                text="Cancel"
                press="onCancelInEditVehicleDialog"
                type="Negative"
            />
        </endButton>
        <content>
            <form:SimpleForm
                id="idStarDesstanmidfglsForm"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanXL="3"
                labelSpanL="3"
                labelSpanM="3"
                labelSpanS="12"
                adjustLabelSpan="false"
                emptySpanXL="4"
                emptySpanL="4"
                emptySpanM="4"
                emptySpanS="0"
                columnsXL="1"
                columnsL="1"
                columnsM="1"
                singleContainerFullSize="false"
            >
                <Label
                    id="_IDGenLabel1"
                    text="Truck Type"
                />
                <Input
                    id="idVehInpTruckType"
                    value="{VehModel>/truckType}"
                    editable="false"
                />

                <Label
                    id="_IDGenLabel2"
                    text="Length"
                />
                <Input
                    id="idVehInplength"
                    value="{VehModel>/length}"
                />

                <Label
                    id="_IDGenLabel3"
                    text="Width"
                />
                <Input
                    id="idVehInpWidth"
                   value="{VehModel>/width}"
                />

                <Label
                    id="_IDGenLabel4"
                    text="Height"
                />
                <Input
                    id="idVehInpheight"
                    value="{VehModel>/height}"
                />
                <Label
                    id="_IDGenLabel7"
                    text="Truck Weight"
                />
                <Input
                    id="idVehInptruckweight"
                    value="{VehModel>/truckWeight}"
                />

                <Label
                    id="_IDGenLabel8"
                    text="Capacity"
                />
                <Input
                    id="idVehInpcapacity"
                    value="{VehModel>/capacity}"
                />
                <Label
                    id="_IDGenLabel9"
                    text="Freezed"
                />
                <Select id="idSlectOPt">
                    <core:Item
                        id="idd0"
                        key="Select"
                        text="Select"
                    />
                    <core:Item
                        id="iDs"
                        key="Yes"
                        text="Yes"
                    />
                    <core:Item
                        id="ifDs"
                        key="No"
                        text="No"
                    />
                </Select>
            </form:SimpleForm>
        </content>
    </Dialog>
</core:FragmentDefinition>
